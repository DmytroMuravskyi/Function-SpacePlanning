{
  "$schema": "https://hypar.io/Schemas/Function.json",
  "id": "132f6a14-6e9e-4d6c-be76-fe9ffa23ea0b",
  "name": "Space Planning",
  "description": "Define program uses for spaces.",
  "model_output": "Space Planning Zones",
  "model_dependencies": [
    {
      "name": "Program Requirements",
      "optional": true
    },
    {
      "name": "Conceptual Mass",
      "optional": true
    },
    {
      "name": "Circulation",
      "optional": true
    },
    {
      "name": "Core",
      "optional": true
    },
    {
      "name": "Walls",
      "optional": true
    },
    {
      "name": "Vertical Circulation",
      "optional": true
    },
    {
      "name": "Levels",
      "optional": true
    },
    {
      "name": "Space Planning Zone Hints",
      "autohide": true,
      "optional": true
    }
  ],
  "input_schema": {
    "type": "object",
    "properties": {
      "Old Space Boundaries": {
        "description": "This input is just for migration purposes. If you have an exported model containing space boundaries, you can import it here to pre-load this function with your previous results and continue editing.",
        "$ref": "https://prod-api.hypar.io/schemas/InputData",
        "$hyparFileTypes": [
          "json"
        ]
      }
    }
  },
  "overrides": {
    "Level Layout": {
      "context": "[*discriminator=Elements.LevelLayout]",
      "identity": {
        "Add Id": {
          "type": "string"
        }
      },
      "schema": {
        "Profiles": {
          "$hyparRelativeToTransform": true,
          "type": "array",
          "items": {
            "$ref": "https://prod-api.hypar.io/schemas/Profile"
          }
        }
      }
    },
    "Program Assignment": {
      "perProperty": true,
      "context": "[*discriminator=Elements.SpaceBoundary]",
      "identity": {
        "Level Add Id": {
          "type": "string"
        },
        "Relative Position": {
          "$ref": "https://hypar.io/Schemas/Geometry/Vector3.json"
        }
      },
      "schema": {
        "Program Type": {
          "description": "What program should be assigned to this zone?",
          "type": [
            "string",
            "null"
          ],
          "$hyparAutocomplete": [
            "Open Office",
            "Private Office",
            "Lounge",
            "Classroom",
            "Pantry",
            "Meeting Room",
            "Phone Booth",
            "Support",
            "Reception",
            "Open Collaboration",
            "Data Hall"
          ],
          "$hyparAutocompleteQuery": {
            "dependency": "Program Requirements",
            "query": "[*discriminator=Elements.ProgramRequirement].Qualified Program Name"
          }
        },
        "Height": {
          "description": "The height of this space.",
          "type": [
            "number",
            "null"
          ],
          "$hyparUnitType": "length",
          "minimum": 0.01,
          "multipleOf": 0.1
        }
      }
    }
  },
  "messages": {
    "en": {
      "overrides": {
        "Program Assignment": {
          "name": "Space Settings"
        },
        "Level Layout": {
          "schema": {
            "Profiles": {
              "name": "Spaces"
            }
          }
        }
      }
    }
  },
  "language": "C#",
  "element_types": [
    "https://prod-api.hypar.io/schemas/SpaceBoundary",
    "https://prod-api.hypar.io/schemas/LevelVolume",
    "https://prod-api.hypar.io/schemas/Level",
    "https://prod-api.hypar.io/schemas/CirculationSegment",
    "https://prod-api.hypar.io/schemas/ServiceCore",
    "https://prod-api.hypar.io/schemas/ProgramRequirement",
    "https://prod-api.hypar.io/schemas/LevelElements",
    "https://prod-api.hypar.io/schemas/ConceptualMass",
    "https://prod-api.hypar.io/schemas/LevelLayout",
    "https://prod-api.hypar.io/schemas/ViewScope"
  ],
  "repository_url": "https://github.com/hypar-io/function",
  "last_updated": "0001-01-01T00:00:00",
  "cli_version": "1.2.0-alpha.12"
}